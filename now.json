{
  "version": 2,
  "name": "myblog",
  "alias": "myblog.now.sh",
  "builds": [
    {
      "src": "./dist/**/*.js",
      "use": "@now/node"
    },
    {
      "src": "*/index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ]
}
