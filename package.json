{
  "name": "myblog",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node dist/index.js",
    "build:client": "webpack --config webpack/webpack.config.prod.js",
    "build:server": "BABEL_ENV=production babel src -s -D -d dist",
    "build": "npm run build:client && npm run build:server",
    "dev:client": "webpack --config webpack/webpack.config.dev.js -w",
    "dev:server": "NODE_ENV=development nodemon src/index.js  --exec babel-node",
    "wait": "wait-on http://localhost:8888",
    "dev": "concurrently \"npm run dev:server\" \"npm run wait && npm run dev:client\""
  },
  "nodemonConfig": {
    "ignore": [
      "./webpack",
      "./dist"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "highlight.js": "^9.13.1",
    "lodash": "^4.17.11",
    "mongoose": "^5.4.4",
    "morgan": "^1.9.1",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-highlight": "^0.12.0",
    "react-markdown": "^4.0.6",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-transform-assets": "^1.0.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-minify": "^0.5.0",
    "browser-sync": "^2.26.3",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "file-loader": "^3.0.1",
    "node-sass": "^4.11.0",
    "nodemon": "1.12.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.1",
    "url-loader": "^1.1.2",
    "wait-on": "^3.2.0",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
  }
}
